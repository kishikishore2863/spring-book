<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="bmw" class="master.di.setterInjection.Bmw"/>
    <bean id="ragerover" class="master.di.setterInjection.RangeRover"/>


    <!-- Inject one of them into TargetClass via constructor -->
    <bean id="showRoom" class="master.di.setterInjection.ShowRoom">
        <property name="car" ref="bmw"/>
    </bean>

</beans>
